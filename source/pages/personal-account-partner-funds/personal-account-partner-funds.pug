				.header__top-authorization
					.header__top-authorization-item
						i.fa.fa-user
						a(href="#") 1241423425
					.header__top-authorization-item
						i.fa.fa-reply
						a(href="#") Выйти
				.select
					.slct_arrow
						i.fa.fa-angle-down
					a(href="#" class="slct") RU
					ul.drop
						li EN
						li UA
						li RU
			.row
				.header__logo.col-lg-3
					a(href="/" class="logo" title="Логотип")
						img(src=require("../../img/logo.png"))
				nav.header__nav
					ul
						-var links = [{title:'Правила обмена', href:'#'},{title:'Партнерам', href:'#'},{title:'Информация', href:'#'},{title:'Контакты', href:'#'}];
						each i in links
							li
								a(href=i.href)= i.title
